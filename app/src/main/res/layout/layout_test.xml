                                android:text="@string/layouttest_text"
                                android:id="@+id/textView38"
                                style="@style/TextPanel3"/>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/panel"
                    android:elevation="6dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp">

                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/imageView10"
                            android:background="@drawable/test"
                            android:layout_marginLeft="@dimen/otstup_krayimage"
                            android:layout_marginRight="@dimen/otstup_krayimage"
                            android:layout_gravity="center"/>
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/otstup_krayimage"
                            android:layout_marginBottom="@dimen/otstup_krayimage">

                        <TextView
