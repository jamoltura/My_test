                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/panel"
                    android:elevation="6dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp">

                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/test"
                            android:id="@+id/imageView11"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/otstup_krayimage"
                            android:layout_marginRight="@dimen/otstup_krayimage"/>
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/otstup_krayimage"
                            android:layout_marginBottom="@dimen/otstup_krayimage">
                        <TextView
                                android:text="AXBOROTNOMA (1996-2003)"
                                android:id="@+id/textView31"
                                style="@style/TextPanel2"/>
                        <TextView
